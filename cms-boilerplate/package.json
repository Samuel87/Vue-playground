{
    "name": "vue-cms-boilerplate",
    "version": "1.0.0",
    "private": true,
    "sasslintConfig": "sass-lint.yml",
    "eslintConfig": ".eslintrc.js",
    "scripts": {
        "watch": "cross-env NODE_ENV=development webpack --watch",
        "build": "cross-env NODE_ENV=production webpack --progress --hide-modules -p",
        "remove:dist": "rm -rf ./dist",
        "remove:modules": "rm -rf ./node_modules"
    },
    "postcss": {
        "plugins": {
            "autoprefixer": {}
        }
    },
    "browserslist": [
        "> 1%",
        "last 2 versions",
        "not ie <= 8"
    ],
    "devDependencies": {
        "@babel/core": "^7.4.0",
        "@babel/preset-env": "^7.4.2",
        "babel-loader": "^8.0.5",
        "cross-env": "^5.0.5",
        "css-loader": "^2.1.1",
        "eslint": "^5.16.0",
        "file-loader": "^3.0.1",
        "mini-css-extract-plugin": "^0.5.0",
        "node-sass": "^4.10.0",
        "postcss-loader": "^3.0.0",
        "sass-lint": "^1.13.1",
        "sass-loader": "^7.1.0",
        "style-loader": "^0.23.1",
        "webpack": "^4.29.6",
        "webpack-cli": "^3.3.0",
        "webpack-shell-plugin": "^0.5.0"
    },
    "dependencies": {
        "@babel/polyfill": "^7.4.0",
        "autoprefixer": "^9.5.0",
        "core-js": "^3.0.0"
    }
}
